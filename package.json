{
  "name": "starter-kit-template",
  "version": "1.0.0",
  "main": "main.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc && shx chmod +x dist/**/*.js",
    "start": "node dist/main.js",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint ./src/**/*.ts --fix",
    "format": "prettier --write \"**/*.{ts,js,cjs,json,md}\"",
    "format:check": "prettier -c .prettierrc --check \"**/*.{ts,js,cjs,json,md}\""
  },
  "keywords": [
    "MCP",
    "MonetizedMCP"
  ],
  "author": "Yunkai",
  "license": "ISC",
  "description": "Starter Kit Template for Monetized MCP",
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "monetizedmcp-sdk": "file:monetizedmcp-sdk-0.1.23.tgz",
    "shx": "^0.4.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "bugs": {
    "url": "https://github.com/MonetizedMCP/typescript-sdk/issues"
  },
  "homepage": "https://github.com/MonetizedMCP/typescript-sdk#readme"
}
